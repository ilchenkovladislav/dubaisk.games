{
  "name": "Dubaisk.games",
  "description": "Упрощает поиск игр для своих друзей",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon2.png"
  },
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": [
    "http://localhost:3000/",
    "http://127.0.0.1:3000/",
    "http://192.168.0.1:3000/"
  ]
}
